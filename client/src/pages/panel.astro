---
import Layout from "../components/Layout.astro";
import UserInfo from '../components/UserInfo.astro';
import { PanelContainer } from "../ReactComponents/panelContainer/panelContainer";
---

<Layout>
    <UserInfo client:load />
    <PanelContainer client:load/>
</Layout>

<script>
    if(!localStorage.getItem("username")) {
        window.location.href = '/login';
    }
</script>

<style>
    body {
        background-color: #2c1d33;
    }
</style>